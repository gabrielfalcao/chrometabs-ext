{
    "manifest_version": 2,
    "name": "tab-sandbox",
    "description": "Easily copy google chrome tabs",
    "version": "1.0",
    "browser_action": {
        "default_icon": "tabicon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "clipboardWrite",
        "tabs",
        "history",
        "management"
    ]
}
